{
  "projectName": "comfyui_yunlanfy",
  "framework": "python",
  "additionalFileExtensions": [".json", ".js"],
  "linters": {
    "python": {
      "enabled": true,
      "plugins": ["flake8", "black"]
    },
    "javascript": {
      "enabled": true,
      "plugins": ["eslint"]
    }
  },
  "formatters": {
    "python": {
      "enabled": true,
      "formatOnSave": true,
      "formatter": "black"
    },
    "javascript": {
      "enabled": true,
      "formatOnSave": true
    }
  },
  "folderStructure": {
    "nodes": {
      "description": "Python modules containing custom node functionality"
    },
    "js": {
      "description": "JavaScript files for frontend UI and interaction"
    },
    "__pycache__": {
      "ignore": true
    },
    ".git": {
      "ignore": true
    }
  },
  "snippets": {
    "python": {
      "newNode": {
        "prefix": "newnode",
        "body": [
          "class $1:",
          "\t@classmethod",
          "\tdef INPUT_TYPES(s):",
          "\t\treturn {",
          "\t\t\t\"required\": {",
          "\t\t\t\t\"$2\": (\"$3\",),",
          "\t\t\t},",
          "\t\t\t\"optional\": {",
          "\t\t\t\t\"$4\": (\"$5\", {\"default\": $6}),",
          "\t\t\t}",
          "\t\t}",
          "\t",
          "\tOUTPUT_NODE = True",
          "\t",
          "\tCATEGORY = \"$7\"",
          "\t",
          "\tdef execute(self, $8):",
          "\t\t$9",
          "\t\treturn {\"$10\": $11}"
        ],
        "description": "Create a new ComfyUI node class"
      },
      "registerNode": {
        "prefix": "registernode",
        "body": [
          "NODE_CLASS_MAPPINGS = {",
          "\t\"$1\": $2",
          "}",
          "",
          "NODE_DISPLAY_NAME_MAPPINGS = {",
          "\t\"$1\": \"$3\"",
          "}"
        ],
        "description": "Register ComfyUI node"
      }
    },
    "javascript": {
      "newWidget": {
        "prefix": "newwidget",
        "body": [
          "class $1 {",
          "\tconstructor() {",
          "\t\tthis.name = \"$2\";",
          "\t}",
          "",
          "\tdraw(ctx, node, widget_width, y, height) {",
          "\t\t$3",
          "\t}",
          "",
          "\tmouse_down(e, pos, node) {",
          "\t\t$4",
          "\t}",
          "}"
        ],
        "description": "Create a new ComfyUI frontend component"
      }
    }
  },
  "codeActions": {
    "python": {
      "detectNodeClass": {
        "pattern": "class (\\w+).*?CATEGORY = ['\"]([^'\"]+)['\"]",
        "action": "Apply ComfyUI node class pattern"
      }
    }
  },
  "templates": {
    "newNodeFile": {
      "path": "nodes/template_node.py",
      "name": "ComfyUI New Node Template"
    }
  }
} 